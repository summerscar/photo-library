{
  "name": "photo-library",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "generate": "node bin/photos2json.js",
    "compress": "node bin/compress.js",
    "photos": "yarn generate && yarn compress",
    "deploy": "yarn build && scp -rP 22 ./dist/* root@173.82.114.233:/www/wwwroot/photos.summerscar.me/",
    "gh-publish": "cross-env TYPE=github vue-cli-service build && gh-pages -d dist"
  },
  "dependencies": {
    "color-thief": "^2.2.4",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "dayjs": "^1.8.14",
    "exif": "^0.6.0",
    "gh-pages": "^2.0.1",
    "imagemagick": "^0.1.3",
    "node-sass": "^4.12.0",
    "ora": "^3.4.0",
    "sass-loader": "^7.1.0",
    "throttle-debounce": "^2.1.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
